generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Secret {
  id          String   @id @default(cuid())
  content     String
  iv          String
  password    String?
  passwordIv  String?
  expiryTime  DateTime
  createdAt   DateTime @default(now())
}